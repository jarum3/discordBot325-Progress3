{
  "name": "discordbot325",
  "version": "1.2.0",
  "description": "A discord bot designed to assist in role management for a classroom setting.",
  "main": "build/app.js",
  "scripts": {
    "start": "npx ts-node deploy-commands.ts && npx ts-node app.ts",
    "start-prod": "npx tsc && npx shx cp .env ./build/.env && node ./build/deploy-commands.js && node ./build/app.js",
    "start-rapid": "npx ts-node app.ts",
    "docs": "npx typedoc --options typedoc.json",
    "lint": "npx eslint . --ext .ts"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@discordjs/builders": "^1.4.0",
    "@discordjs/rest": "^1.5.0",
    "axios": "^1.2.2",
    "discord-api-types": "^0.37.34",
    "discord-interactions": "^3.2.0",
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.3",
    "events": "^3.3.0",
    "package.json": "^0.0.0",
    "typedoc": "^0.23.25"
  },
  "devDependencies": {
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "shx": "^0.3.4",
    "typedoc-theme-hierarchy": "^3.0.2",
    "typescript": "^4.9.5"
  }
}